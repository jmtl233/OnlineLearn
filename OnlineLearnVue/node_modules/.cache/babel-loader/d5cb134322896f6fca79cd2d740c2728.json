A;AAHA,OADA;AAMA,MAAA,KAAA,EAAA,EANA;AAOA,MAAA,MAAA,EAAA,EAPA;AAQA,MAAA,OAAA,EAAA;AACA,QAAA,YAAA,EAAA;AADA,OARA;AAWA,MAAA,MAAA,EAAA;AACA,QAAA,OAAA,EAAA;AADA,OAXA;AAcA,MAAA,SAAA,EAAA,EAdA;AAgBA,MAAA,KAAA,EAAA;AAhBA,KAAA;AAmBA,GAtBA;AAuBA,EAAA,OAvBA,qBAuBA;AACA,SAAA,IAAA,CAAA,OAAA,GAAA,OAAA,CAAA,GAAA,CAAA,SAAA,CAAA;AACA,SAAA,MAAA,CAAA,OAAA,GAAA,OAAA,CAAA,GAAA,CAAA,SAAA,CAAA;AACA,SAAA,aAAA,CAAA,KAAA,MAAA;AACA,SAAA,oBAAA,CAAA,KAAA,IAAA;AACA,GA5BA;AA6BA,EAAA,OAAA,EAAA;AAGA,IAAA,WAHA,uBAGA,KAHA,EAGA;AAAA;;AACA,WAAA,OAAA,CAAA,YAAA,GAAA,KAAA;AACA,MAAA,MAAA,CAAA,KAAA,MAAA,CAAA,CAAA,IAAA,CAAA,UAAA,IAAA,EAAA;AACA,QAAA,KAAA,CAAA,MAAA,GAAA,IAAA,CAAA,IAAA,CAAA,UAAA,CAAA,IAAA;AAEA,OAHA;AAIA,KATA;AAUA,IAAA,gBAVA,4BAUA,IAVA,EAUA;AACA,WAAA,IAAA,CAAA,QAAA,GAAA,IAAA;AACA,WAAA,oBAAA,CAAA,KAAA,IAAA,EAFA,CAGA;;AAEA,MAAA,OAAA,CAAA,GAAA,wBAAA,IAAA;AACA,KAhBA;AAiBA,IAAA,mBAjBA,+BAiBA,OAjBA,EAiBA;AACA,WAAA,IAAA,CAAA,IAAA,GAAA,OAAA;AACA,WAAA,oBAAA,CAAA,KAAA,IAAA;AACA,MAAA,OAAA,CAAA,GAAA,+BAAA,OAAA;AACA,KArBA;AAuBA,IAAA,aAvBA,yBAuBA,IAvBA,EAuBA;AAAA;;AACA,MAAA,YAAA,CAAA,IAAA,CAAA,CAAA,IAAA,CAAA,UAAA,IAAA,EAAA;AACA,QAAA,MAAA,CAAA,SAAA,GAAA,IAAA,CAAA,IAAA,CAAA,UAAA;AACA,OAFA;AAGA,KA3BA;AA4BA,IAAA,oBA5BA,gCA4BA,IA5BA,EA4BA;AAAA;;AAEA,UAAA,OAAA,CAAA,GAAA,CAAA,SAAA,KAAA,WAAA,EAAA;AACA;AACA,OAFA,MAEA;AACA,QAAA,IAAA,CAAA,OAAA,GAAA,OAAA,CAAA,GAAA,CAAA,SAAA,CAAA;AACA;;AACA,MAAA,SAAA,CAAA,IAAA,CAAA,CAAA,IAAA,CAAA,UAAA,IAAA,EAAA;AACA,QAAA,MAAA,CAAA,KAAA,GAAA,IAAA,CAAA,IAAA,CAAA,UAAA,CAAA,OAAA;AACA,QAAA,MAAA,CAAA,KAAA,GAAA,IAAA,CAAA,IAAA,CAAA,UAAA,CAAA,KAAA;AACA,OAHA;AAIA;AAvCA;AA7BA,CAAA","sourcesContent":["<template>\r\n    <div>\r\n        <div class=\"dem\">\r\n            <div v-if=\"iData.length == 0\" style=\"font-size: 26px;color: grey\">\r\n                - 当前未加入班级或者任教老师未发布视频 -\r\n            </div>\r\n            <div class='demo' v-for=\"url in iData\" :key=\"url.id\">\r\n                <div>\r\n                    <router-link\r\n                        :to=\"{ path: '/detailonlineweb', query: { videoTotalId: url.id, userId: url.userId } }\">\r\n                        <el-image style=\"height: 120px\" :src=\"$store.state.baseApi + url.coverUrl\"></el-image>\r\n                    </router-link>\r\n                    <h3>{{ url.topic }}</h3>\r\n                    <div class=\"ta\">\r\n                        {{ url.userName }}\r\n                        <!-- <el-tag> {{url.createTime}}</el-tag> -->\r\n                    </div>\r\n\r\n\r\n                </div>\r\n\r\n            </div>\r\n\r\n        </div>\r\n        <div style=\"height: 40px\"></div>\r\n        <el-pagination @size-change=\"handleSizeChange\" @current-change=\"handleCurrentChange\" :current-page=\"page.page\"\r\n            :page-sizes=\"[10, 20, 30, 40]\" :page-size=\"page.pageSize\" layout=\"total, sizes, prev, pager, next, jumper\"\r\n            :total=\"total\">\r\n        </el-pagination>\r\n    </div>\r\n</template>\r\n<script>\r\nimport { onlineweb, onlinecourse } from '../../../api/studentweb/online.js'\r\nimport { videos } from \"../../../api/studentweb/video.js\";\r\nimport Cookies from \"js-cookie\";\r\nexport default {\r\n    name: \"OnLineLearn\",\r\n    data() {\r\n        return {\r\n            page: {\r\n                page: 1, //初始页\r\n                pageSize: 10,    //    每页的数据\r\n                classId: ''\r\n            },\r\n            iData: [],\r\n            videos: [],\r\n            videoss: {\r\n                videoTotalId: ''\r\n            },\r\n            course: {\r\n                classId: ''\r\n            },\r\n            onLineCou: [],\r\n\r\n            total: 0\r\n\r\n        }\r\n    },\r\n    created() {\r\n        this.page.classId = Cookies.get('classId')\r\n        this.course.classId = Cookies.get('classId')\r\n        this.listAllCourse(this.course)\r\n        this.listAllStudentsScore(this.page)\r\n    },\r\n    methods: {\r\n\r\n\r\n        detailvideo(video) {\r\n            this.videoss.videoTotalId = video\r\n            videos(this.videos).then(resp => {\r\n                this.videos = resp.data.resultData.data\r\n\r\n            })\r\n        },\r\n        handleSizeChange(size) {\r\n            this.page.pageSize = size;\r\n            this.listAllStudentsScore(this.page)\r\n            // console.log(this.pageSize,'888')\r\n\r\n            console.log(`每页 ${size} 条`);\r\n        },\r\n        handleCurrentChange(pageNum) {\r\n            this.page.page = pageNum;\r\n            this.listAllStudentsScore(this.page)\r\n            console.log(`当前页: ${pageNum}`);\r\n        },\r\n\r\n        listAllCourse(page) {\r\n            onlinecourse(page).then(resp => {\r\n                this.onLineCou = resp.data.resultData\r\n            })\r\n        },\r\n        listAllStudentsScore(page) {\r\n\r\n            if (Cookies.get('classId') == 'undefined') {\r\n                return;\r\n            } else {\r\n                page.classId = Cookies.get('classId')\r\n            }\r\n            onlineweb(page).then(resp => {\r\n                this.iData = resp.data.resultData.records\r\n                this.total = resp.data.resultData.total\r\n            })\r\n        }\r\n    }\r\n\r\n}\r\n</script>\r\n<style scoped>\r\n.el-image__inner {\r\n    vertical-align: top;\r\n    width: 200px;\r\n    height: 100px;\r\n}\r\n\r\n.demo {\r\n    width: 255px;\r\n    text-align: center;\r\n    line-height: 15%;\r\n    border: 1px solid lightgrey;\r\n    border-radius: 4px;\r\n    padding: 25px 0 0 0;\r\n\r\n}\r\n\r\nel-button {\r\n    display: inline-block;\r\n}\r\n\r\n.course {\r\n    display: inline-block;\r\n}\r\n\r\n.ta {\r\n    display: flex;\r\n    justify-content: space-between;\r\n    height: 30px;\r\n    line-height: 30px;\r\n}\r\n\r\n.dem {\r\n    display: flex;\r\n    justify-content: flex-start;\r\n\r\n    flex-wrap: wrap;\r\n}\r\n</style>"],"sourceRoot":"src/views/studentweb/onlinelearn"}]}
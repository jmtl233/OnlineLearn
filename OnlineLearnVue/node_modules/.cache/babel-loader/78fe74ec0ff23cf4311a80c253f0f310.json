                // this.$store.dispatch('addNotHomework',NotHomeWork)\r\n            }\r\n        }\r\n    }\r\n</script>\r\n\r\n<style scoped>\r\n.bt{\r\n    display: flex;\r\n    justify-content: center;\r\n}\r\n    .dohomework{\r\n        display: flex;\r\n        justify-content: center;\r\n    }\r\n</style>"],"sourceRoot":"src/views/studentweb/homeworkmanagement/checkhomework"}]}
,EAAA,OAhBA,qBAgBA;AACA,SAAA,IAAA,CAAA,MAAA,GAAA,OAAA,CAAA,GAAA,CAAA,QAAA,CAAA;AACA,SAAA,IAAA,CAAA,MAAA,GAAA,OAAA,CAAA,GAAA,CAAA,QAAA,CAAA;AACA,SAAA,WAAA,CAAA,KAAA,IAAA;AACA,GApBA;AAqBA,EAAA,OAAA,EAAA;AACA,IAAA,aADA,yBACA,KADA,EACA,GADA,EACA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA;AACA,QAAA,IAAA,EAAA,SADA;AAEA,QAAA,MAAA,EAAA;AACA,UAAA,KAAA,EAAA;AADA;AAFA,OAAA;AAMA,KARA;AAUA,IAAA,eAVA,2BAUA,KAVA,EAUA,GAVA,EAUA;AAAA;;AACA,WAAA,KAAA,CAAA,EAAA,GAAA,GAAA,CAAA,EAAA;AACA,MAAA,UAAA,CAAA,KAAA,KAAA,CAAA,CAAA,IAAA,CAAA,UAAA,IAAA,EAAA;AACA,YAAA,IAAA,CAAA,IAAA,CAAA,IAAA,IAAA,GAAA,EAAA;AACA,UAAA,KAAA,CAAA,QAAA,CAAA;AACA,YAAA,OAAA,EAAA,WADA;AAEA,YAAA,IAAA,EAAA;AAFA,WAAA;;AAIA,UAAA,KAAA,CAAA,WAAA,CAAA,KAAA,CAAA,IAAA;AACA,SANA,MAMA;AACA,UAAA,KAAA,CAAA,QAAA,CAAA,KAAA,CAAA,MAAA;AACA;AACA,OAVA;AAWA,KAvBA;AAwBA,IAAA,WAxBA,uBAwBA,IAxBA,EAwBA;AAAA;;AACA,MAAA,QAAA,CAAA,IAAA,CAAA,CAAA,IAAA,CAAA,UAAA,IAAA,EAAA;AACA,QAAA,MAAA,CAAA,QAAA,GAAA,IAAA,CAAA,IAAA,CAAA,UAAA,CAAA,IAAA;AACA,OAFA;AAGA,KA5BA;AA6BA,IAAA,gBA7BA,4BA6BA,IA7BA,EA6BA;AACA,WAAA,IAAA,CAAA,QAAA,GAAA,IAAA,CADA,CAEA;;AAEA,MAAA,OAAA,CAAA,GAAA,wBAAA,IAAA;AACA,KAlCA;AAmCA,IAAA,mBAnCA,+BAmCA,OAnCA,EAmCA;AACA,WAAA,IAAA,CAAA,OAAA,GAAA,OAAA,CADA,CAEA;;AACA,MAAA,OAAA,CAAA,GAAA,+BAAA,OAAA;AACA;AAvCA;AArBA,CAAA","sourcesContent":["<template>\r\n   <div>\r\n      <router-link to=\"/pdetailteacher\"><el-button type=\"primary\" > 发布知识点</el-button></router-link>\r\n       <el-table\r\n               :data=\"knowData\"\r\n               style=\"width: 100%\">\r\n           <el-table-column type=\"expand\">\r\n               <template slot-scope=\"props\">\r\n                   <el-form label-position=\"left\" inline class=\"demo-table-expand\">\r\n                       <el-form-item label=\"班级：\">\r\n                           <span>{{ props.row.className }}</span>\r\n                       </el-form-item>\r\n                       <el-form-item label=\"发布人：\">\r\n                           <span>{{ props.row.userName }}</span>\r\n                       </el-form-item>\r\n                       <el-form-item label=\"科目：\">\r\n                           <span>{{ props.row.subjectName }}</span>\r\n                       </el-form-item>\r\n                       <el-form-item label=\"主题：\">\r\n                           <span>{{ props.row.title }}</span>\r\n                       </el-form-item>\r\n                       <el-form-item label=\"知识点补充内容：\">\r\n                           <span>{{ props.row.content }}</span>\r\n                       </el-form-item>\r\n                       <el-form-item label=\"发布时间：\">\r\n                           <span>{{ props.row.createTime }}</span>\r\n                       </el-form-item>\r\n<!--                       <el-form-item label=\"商品描述\">-->\r\n<!--                           <span>{{ props.row.desc }}</span>-->\r\n<!--                       </el-form-item>-->\r\n                   </el-form>\r\n               </template>\r\n           </el-table-column>\r\n           <el-table-column\r\n                   label=\"详情内容\"\r\n                   prop=\"subjectName\">\r\n           </el-table-column>\r\n           <el-table-column\r\n                   label=\"发布时间\"\r\n                   prop=\"createTime\">\r\n           </el-table-column>\r\n           <el-table-column\r\n                   label=\"发布主题\"\r\n                   prop=\"title\">\r\n           </el-table-column>\r\n           <el-table-column\r\n                   label=\"科目\"\r\n                   prop=\"subjectName\">\r\n           </el-table-column>\r\n           <el-table-column\r\n                   label=\"班级\"\r\n                   prop=\"className\">\r\n           </el-table-column>\r\n           <el-table-column label=\"操作\">\r\n               <template slot-scope=\"scope\">\r\n                   <el-button\r\n                           size=\"mini\"\r\n                           @click=\"EditKnowPoint(scope.$index, scope.row)\">编辑</el-button>\r\n                   <el-button\r\n                           size=\"mini\"\r\n                           type=\"danger\"\r\n                           @click=\"DeleteKnowPoint(scope.$index, scope.row)\">删除</el-button>\r\n               </template>\r\n           </el-table-column>\r\n       </el-table>\r\n\r\n       <el-pagination\r\n               @size-change=\"handleSizeChange\"\r\n               @current-change=\"handleCurrentChange\"\r\n               :current-page=\"page.page\"\r\n               :page-sizes=\"[10, 20, 30, 40]\"\r\n               :page-size=\"page.pageSize\"\r\n               layout=\"total, sizes, prev, pager, next, jumper\"\r\n               :total=\"knowData.length\">\r\n       </el-pagination>\r\n   </div>\r\n</template>\r\n\r\n<script>\r\n    import {listKnow,deleteKnow} from \"../../api/personal.js\";\r\n    import Cookies from 'js-cookie'\r\n    export default {\r\n        name: \"PersonalInfo\",\r\n        data(){\r\n            return{\r\n                page:{\r\n                    page:1, //初始页\r\n                    pageSize:10,    //    每页的数据\r\n                    userId:'',\r\n                    roleId:''\r\n                },\r\n                knowData:[],\r\n                param:{\r\n                    id:'',\r\n                }\r\n            }\r\n        },\r\n        created() {\r\n            this.page.userId=Cookies.get('userId')\r\n            this.page.roleId=Cookies.get('roleId')\r\n            this.listAllKnow(this.page)\r\n        },\r\n        methods:{\r\n            EditKnowPoint(index,row){\r\n              this.$router.push({\r\n                  name:'PDetail',\r\n                  params:{\r\n                      data1:row\r\n                  }\r\n              })\r\n            },\r\n\r\n            DeleteKnowPoint(index,row){\r\n                this.param.id=row.id\r\n                deleteKnow(this.param).then(resp=>{\r\n                    if(resp.data.code==200){\r\n                        this.$message({\r\n                            message: '恭喜你，删除成功 ',\r\n                            type: 'success'\r\n                        });\r\n                        this.listAllKnow(this.page)\r\n                    }else{\r\n                        this.$message.error('删除失败');\r\n                    }\r\n                })\r\n            },\r\n            listAllKnow(page){\r\n                listKnow(page).then(resp=>{\r\n                    this.knowData=resp.data.resultData.data\r\n                })\r\n            },\r\n            handleSizeChange(size) {\r\n                this.page.pageSize = size;\r\n                // console.log(this.pageSize,'888')\r\n\r\n                console.log(`每页 ${size} 条`);\r\n            },\r\n            handleCurrentChange(pageNum) {\r\n                this.page.pageNum = pageNum;\r\n                // this.listAllStudentsScore(this.page)\r\n                console.log(`当前页: ${pageNum}`);\r\n            },\r\n\r\n        }\r\n    }\r\n</script>\r\n\r\n<style scoped>\r\n    .demo-table-expand {\r\n        font-size: 0;\r\n    }\r\n    .demo-table-expand label {\r\n        width: 90px;\r\n        color: #99a9bf;\r\n    }\r\n    .demo-table-expand .el-form-item {\r\n        margin-right: 0;\r\n        margin-bottom: 0;\r\n        width: 50%;\r\n    }\r\n</style>\r\n"],"sourceRoot":"src/views/personalInfo"}]}
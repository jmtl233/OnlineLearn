,YAAA,IAAA,CAAA,IAAA,CAAA,IAAA,IAAA,GAAA,EAAA;AACA,UAAA,KAAA,CAAA,QAAA,CAAA,OAAA,CAAA,QAAA;;AACA,UAAA,KAAA,CAAA,iBAAA,GAAA,KAAA;;AACA,UAAA,KAAA,CAAA,YAAA,CAAA,KAAA,CAAA,IAAA;AACA,SAJA,MAIA;AACA,UAAA,KAAA,CAAA,QAAA,CAAA,KAAA,CAAA,OAAA;AACA;AACA,OARA;AASA,KAnBA;AAoBA,IAAA,MApBA,oBAoBA;AACA,MAAA,OAAA,CAAA,MAAA,CAAA,QAAA;AACA,MAAA,OAAA,CAAA,MAAA,CAAA,SAAA;AACA,MAAA,OAAA,CAAA,MAAA,CAAA,QAAA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA,QAAA;AACA,WAAA,QAAA,CAAA,OAAA,CAAA,MAAA;AACA;AA1BA;AArBA,CAAA","sourcesContent":["<template>\r\n    <div>\r\n        <el-menu\r\n            :default-active=\"'1'\"\r\n            mode=\"horizontal\"\r\n            class=\"custom-menu\"\r\n            style=\"line-height: 60px\">\r\n            <div class=\"cn\">\r\n                <div class=\"title\">\r\n                    网上学堂在线平台（教师）\r\n                </div>\r\n\r\n                <div class=\"blockl\">\r\n                    <el-submenu index=\"2\">\r\n                        <template slot=\"title\">\r\n                            <!-- 使用Element UI内置图标 -->\r\n                            <i class=\"el-icon-user\" style=\"font-size: 28px; color: #D84315; margin-right: 15px;\"></i>\r\n                        </template>\r\n                        <el-menu-item index=\"/checkhomework\" @click=\"logout()\">退出</el-menu-item>\r\n                        <el-menu-item @click=\"change()\">修改密码</el-menu-item>\r\n                    </el-submenu>\r\n                </div>\r\n            </div>\r\n        </el-menu>\r\n\r\n        <div>\r\n            <el-dialog title=\"修改密码\" :visible.sync=\"dialogFormVisible\" width=\"30%\" :before-close=\"handleClose\">\r\n                <el-input placeholder=\"请输入原密码\" v-model=\"changePassword.password\" show-password></el-input>\r\n                <p>\r\n                    <el-input placeholder=\"请输入新密码\" v-model=\"changePassword.newPassword\" show-password></el-input>\r\n                </p>\r\n                <span slot=\"footer\" class=\"dialog-footer\">\r\n          <el-button @click=\"dialogFormVisible = false\">取 消</el-button>\r\n          <el-button type=\"primary\" @click=\"submit(changePassword)\">确 定</el-button>\r\n        </span>\r\n            </el-dialog>\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport Cookies from \"js-cookie\";\r\nimport { password } from '../../../api/personal.js'\r\n\r\nexport default {\r\n    name: \"Header\",\r\n    data() {\r\n        return {\r\n            changePassword: {\r\n                password: '',\r\n                newPassword: '',\r\n                id: ''\r\n            },\r\n            dialogFormVisible: false,\r\n            info: {\r\n                password: '',\r\n                newPassword: '',\r\n                id: ''\r\n            },\r\n            drawer: false,\r\n        }\r\n    },\r\n    created() {\r\n        this.changePassword.id = Cookies.get(\"userId\")\r\n    },\r\n    methods: {\r\n        handleClose(done) {\r\n            this.$confirm('确认关闭？')\r\n                .then(_ => { done() })\r\n                .catch(_ => {})\r\n        },\r\n        change() {\r\n            this.dialogFormVisible = true\r\n        },\r\n        submit(da) {\r\n            password(da).then(resp => {\r\n                if (resp.data.code == 200) {\r\n                    this.$message.success('密码修改成功')\r\n                    this.dialogFormVisible = false\r\n                    this.studentquery(this.page)\r\n                } else {\r\n                    this.$message.error('原密码错误')\r\n                }\r\n            })\r\n        },\r\n        logout() {\r\n            Cookies.remove('userId')\r\n            Cookies.remove('classId')\r\n            Cookies.remove('roleId')\r\n            this.$router.push('/login')\r\n            this.$message.success('退出成功')\r\n        }\r\n    }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n/* 保持原有字体不变 */\r\n.cn {\r\n    display: flex;\r\n    justify-content: space-between;\r\n}\r\n\r\n/* 导航栏背景设置 */\r\n.custom-menu {\r\n    background: linear-gradient(160deg, #FFF3E0 0%, #FFE0B2 100%) !important;\r\n    border-bottom: none !important;\r\n}\r\n\r\n.title {\r\n    line-height: 60px;\r\n    padding-left: 20px;\r\n    font-weight: bold;\r\n    color: #EF6C00;\r\n    font-size: 24px;\r\n    float: left;\r\n}\r\n\r\n.blockl {\r\n    float: right;\r\n    margin-right: 30px;\r\n}\r\n\r\n/* 菜单项样式优化 */\r\n.el-submenu__title {\r\n    height: 60px !important;\r\n    line-height: 60px !important;\r\n}\r\n\r\n.el-submenu__title:hover {\r\n    background-color: rgba(255, 255, 255, 0.3) !important;\r\n}\r\n\r\n.el-icon-user {\r\n    vertical-align: middle;\r\n    transition: transform 0.2s;\r\n}\r\n\r\n.el-icon-user:hover {\r\n    transform: scale(1.1);\r\n}\r\n</style>"],"sourceRoot":"src/views/container/header"}]}
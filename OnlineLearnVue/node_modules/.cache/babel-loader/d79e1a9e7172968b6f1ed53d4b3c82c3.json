APA,KAAA;AASA,GAZA;AAaA,EAAA,OAbA,qBAaA;AACA,SAAA,IAAA,CAAA,MAAA,GAAA,OAAA,CAAA,GAAA,CAAA,QAAA,CAAA;AACA,SAAA,UAAA,CAAA,KAAA,IAAA;AACA,GAhBA;AAiBA,EAAA,OAAA,EAAA;AACA,IAAA,MADA,kBACA,UADA,EACA;AAAA;;AACA,MAAA,UAAA,CAAA,MAAA,GAAA,CAAA;AACA,MAAA,SAAA,CAAA,UAAA,CAAA,CAAA,IAAA,CAAA,UAAA,IAAA,EAAA;AACA,YAAA,IAAA,CAAA,IAAA,CAAA,IAAA,IAAA,GAAA,EAAA;AACA,UAAA,KAAA,CAAA,QAAA,CAAA;AACA,YAAA,OAAA,EAAA,WADA;AAEA,YAAA,IAAA,EAAA;AAFA,WAAA;;AAIA,UAAA,KAAA,CAAA,KAAA,GAAA,KAAA,CAAA,KAAA,GAAA,CAAA;;AACA,UAAA,KAAA,CAAA,UAAA,CAAA,KAAA,CAAA,IAAA;AACA,SAPA,MAOA;AACA,UAAA,KAAA,CAAA,QAAA,CAAA,KAAA,CAAA,MAAA;AACA;AACA,OAXA;AAYA,KAfA;AAgBA,IAAA,UAhBA,sBAgBA,IAhBA,EAgBA;AAAA;;AACA,MAAA,aAAA,CAAA,IAAA,CAAA,CAAA,IAAA,CAAA,UAAA,IAAA,EAAA;AACA,QAAA,MAAA,CAAA,UAAA,GAAA,IAAA,CAAA,IAAA,CAAA,UAAA,CAAA,IAAA;AACA,OAFA;AAGA;AApBA;AAjBA,CAAA","sourcesContent":["<template>\r\n    <div class=\"block\">\r\n        <el-timeline>\r\n\r\n            <div v-for=\"t in shduleData\">\r\n            <el-timeline-item :timestamp=t.createTime placement=\"top\">\r\n                <el-card>\r\n                    <div class=\"cs\">\r\n                        <span><el-tag type=\"success\" >{{t.senderName}}提问:</el-tag></span>\r\n                        <span >\r\n                        <el-tag type=\"warning\">问题来源：{{t.topic}}</el-tag>\r\n                        </span>\r\n\r\n                        <span ><el-tag type=\"success\" >{{t.recipientName}}回答</el-tag></span>\r\n                    </div>\r\n                    <el-divider content-position=\"center\">下面是提问记录</el-divider>\r\n                    <div class=\"csc\">\r\n                        <el-input\r\n                                type=\"textarea\"\r\n                                :rows=\"4\"\r\n                                placeholder=\"请输入内容\"\r\n                                v-model=t.content>\r\n                        </el-input>\r\n                        <el-input\r\n                                type=\"textarea\"\r\n                                :rows=\"4\"\r\n                                placeholder=\"请输入内容\"\r\n                                v-model=t.restore>\r\n                        </el-input>\r\n                    </div>\r\n\r\n                    <el-badge :value=\"count\" class=\"item\">\r\n                        <el-button size=\"small\" @click=\"restor(t)\">回复</el-button>\r\n                    </el-badge>\r\n                </el-card>\r\n            </el-timeline-item>\r\n            </div>\r\n        </el-timeline>\r\n        <el-pagination\r\n                @size-change=\"handleSizeChange\"\r\n                @current-change=\"handleCurrentChange\"\r\n                :current-page=\"page.page\"\r\n                :page-sizes=\"[10, 20, 30, 40]\"\r\n                :page-size=\"page.pageSize\"\r\n                layout=\"total, sizes, prev, pager, next, jumper\"\r\n                :total=\"shduleData.length\">\r\n        </el-pagination>\r\n    </div>\r\n\r\n</template>\r\n\r\n<script>\r\n    import {listAllShdule,addShdule} from \"../../api/shdule\";\r\n    import Cookies from \"js-cookie\";\r\n\r\n    export default {\r\n        name: \"CourseManagement\",\r\n        data() {\r\n            return {\r\n                shduleData: [],\r\n                page:{\r\n                    page:1, //初始页\r\n                    pageSize:10,    //    每页的数据\r\n                    userId:''\r\n                },\r\n                count:0,\r\n            }\r\n        },\r\n        created() {\r\n            this.page.userId=Cookies.get('userId')\r\n            this.listShdule(this.page)\r\n        },\r\n        methods:{\r\n            restor(shduleData){\r\n                shduleData.status=1\r\n                addShdule(shduleData).then(resp=>{\r\n                    if(resp.data.code==200){\r\n                        this.$message({\r\n                            message: '恭喜你，回复成功 ',\r\n                            type: 'success'\r\n                        });\r\n                        this.count=this.count+1\r\n                        this.listShdule(this.page)\r\n                    }else{\r\n                        this.$message.error('回复失败');\r\n                    }\r\n                })\r\n            },\r\n            listShdule(page){\r\n                listAllShdule(page).then(resp=>{\r\n                    this.shduleData=resp.data.resultData.data\r\n                })\r\n            }\r\n        },\r\n    }\r\n</script>\r\n\r\n<style scoped>\r\n.item{\r\n    display: flex;\r\n    justify-content: flex-end;\r\n}\r\n.csc{\r\n    display: flex;\r\n    justify-content: space-between;\r\n}\r\n    .cs{\r\n        display: flex;\r\n        justify-content: space-between;\r\n    }\r\n    h4{\r\n        display: inline !important;\r\n    }\r\n\r\n</style>\r\n"],"sourceRoot":"src/views/coursemanagement"}]}
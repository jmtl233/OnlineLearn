          type=\"primary\"\r\n                icon=\"el-icon-plus\"\r\n                @click=\"addStudent\"\r\n                class=\"btn-primary\"\r\n            >\r\n                新增学生\r\n            </el-button>\r\n        </div>\r\n\r\n        <!-- 数据表格 -->\r\n        <el-table\r\n            :data=\"StudentData\"\r\n            style=\"width: 100%\"\r\n            border\r\n            stripe\r\n            class=\"custom-table\"\r\n        >\r\n            <el-table-column\r\n                prop=\"account\"\r\n                label=\"账号\"\r\n                width=\"160\"\r\n                align=\"center\"\r\n                sortable\r\n            />\r\n            <el-table-column\r\n                prop=\"userName\"\r\n                label=\"姓名\"\r\n                width=\"150\"\r\n                align=\"center\"\r\n            />\r\n            <el-table-column\r\n                prop=\"sexName\"\r\n                label=\"性别\"\r\n                width=\"120\"\r\n                align=\"center\"\r\n            />\r\n            <el-table-column\r\n                prop=\"className\"\r\n                label=\"班级\"\r\n                min-width=\"200\"\r\n                align=\"center\"\r\n            />\r\n            <el-table-column\r\n                label=\"操作\"\r\n                width=\"220\"\r\n                align=\"center\"\r\n                fixed=\"right\"\r\n            >\r\n                <template slot-scope=\"scope\">\r\n                    <el-button\r\n                        type=\"success\"\r\n                        size=\"mini\"\r\n                        @click=\"handleEdit(scope.$index, scope.row)\"\r\n                        class=\"btn-edit\"\r\n                    >\r\n                        编辑\r\n                    </el-button>\r\n                    <el-button\r\n                        type=\"danger\"\r\n                        size=\"mini\"\r\n                        @click=\"handleDelete(scope.row.id)\"\r\n                        class=\"btn-delete\"\r\n                    >\r\n                        删除\r\n                    </el-button>\r\n                </template>\r\n            </el-table-column>\r\n        </el-table>\r\n\r\n        <!-- 分页组件 -->\r\n        <div class=\"pagination-container\">\r\n            <el-pagination\r\n                background\r\n                :current-page=\"page.pageNum\"\r\n                :page-sizes=\"[10, 20, 30, 40]\"\r\n                :page-size=\"page.pageSize\"\r\n                layout=\"total, sizes, prev, pager, next, jumper\"\r\n                :total=\"StudentData.length\"\r\n                @size-change=\"handleSizeChange\"\r\n                @current-change=\"handleCurrentChange\"\r\n            />\r\n        </div>\r\n\r\n        <!-- 新增对话框 -->\r\n        <el-dialog\r\n            title=\"新增学生\"\r\n            :visible.sync=\"dialogFormVisible\"\r\n            width=\"600px\"\r\n            top=\"8vh\"\r\n        >\r\n            <el-form\r\n                ref=\"addForm\"\r\n                :model=\"form\"\r\n                :rules=\"rules\"\r\n                label-position=\"left\"\r\n                label-width=\"100px\"\r\n            >\r\n                <el-form-item label=\"学生姓名\" prop=\"userName\">\r\n                    <el-input\r\n                        v-model=\"form.userEntity.userName\"\r\n                        placeholder=\"请输入学生姓名\"\r\n                        clearable\r\n                    />\r\n                </el-form-item>\r\n                <el-form-item label=\"学生账号\" prop=\"account\">\r\n                    <el-input\r\n                        v-model=\"form.userEntity.account\"\r\n                        placeholder=\"请输入登录账号\"\r\n                        clearable\r\n                    />\r\n                </el-form-item>\r\n                <el-form-item label=\"联系电话\" prop=\"phone\">\r\n                    <el-input\r\n                        v-model=\"form.userEntity.phone\"\r\n                        placeholder=\"请输入11位手机号\"\r\n                        clearable\r\n                    />\r\n                </el-form-item>\r\n                <el-form-item label=\"性别\" prop=\"sex\">\r\n                    <el-select\r\n                        v-model=\"form.userEntity.sex\"\r\n                        placeholder=\"请选择性别\"\r\n                        style=\"width: 100%\"\r\n                    >\r\n                        <el-option label=\"男\" value=\"0\" />\r\n                        <el-option label=\"女\" value=\"1\" />\r\n                    </el-select>\r\n                </el-form-item>\r\n                <el-form-item label=\"所属班级\" prop=\"classId\">\r\n                    <el-select\r\n                        v-model=\"form.classId\"\r\n                        placeholder=\"请选择班级\"\r\n                        style=\"width: 100%\"\r\n                    >\r\n                        <el-option\r\n                            v-for=\"item in queryCla\"\r\n                            :key=\"item.id\"\r\n                            :label=\"item.className\"\r\n                            :value=\"item.id\"\r\n                        />\r\n                    </el-select>\r\n                </el-form-item>\r\n            </el-form>\r\n            <div slot=\"footer\" class=\"dialog-footer\">\r\n                <el-button @click=\"dialogFormVisible = false\">取消</el-button>\r\n                <el-button\r\n                    type=\"primary\"\r\n                    @click=\"submit(form)\"\r\n                    :loading=\"submitLoading\"\r\n                >\r\n                    确认提交\r\n                </el-button>\r\n            </div>\r\n        </el-dialog>\r\n\r\n        <!-- 编辑对话框 -->\r\n        <el-dialog\r\n            title=\"编辑学生信息\"\r\n            :visible.sync=\"dialogFormVisibleEdit\"\r\n            width=\"600px\"\r\n            top=\"8vh\"\r\n        >\r\n            <el-form\r\n                ref=\"updateForm\"\r\n                :model=\"editStudent\"\r\n                label-position=\"left\"\r\n                label-width=\"100px\"\r\n            >\r\n                <el-form-item label=\"学生姓名\">\r\n                    <el-input\r\n                        v-model=\"editStudent.userName\"\r\n                        placeholder=\"请输入学生姓名\"\r\n                        clearable\r\n                    />\r\n                </el-form-item>\r\n                <el-form-item label=\"学生账号\">\r\n                    <el-input\r\n                        v-model=\"editStudent.account\"\r\n                        disabled\r\n                        clearable\r\n                    />\r\n                </el-form-item>\r\n                <el-form-item label=\"性别\">\r\n                    <el-select\r\n                        v-model=\"editStudent.sex\"\r\n                        placeholder=\"请选择性别\"\r\n                        style=\"width: 100%\"\r\n                    >\r\n                        <el-option\r\n                            v-for=\"item in sexAll\"\r\n                            :key=\"item.sex\"\r\n                            :label=\"item.sexName\"\r\n                            :value=\"item.sex\"\r\n                        />\r\n                    </el-select>\r\n                </el-form-item>\r\n                <el-form-item label=\"所属班级\">\r\n                    <el-select\r\n                        v-model=\"editStudent.classId\"\r\n                        placeholder=\"请选择班级\"\r\n                        style=\"width: 100%\"\r\n                    >\r\n                        <el-option\r\n                            v-for=\"item in queryCla\"\r\n                            :key=\"item.id\"\r\n                            :label=\"item.className\"\r\n                            :value=\"item.id\"\r\n                        />\r\n                    </el-select>\r\n                </el-form-item>\r\n            </el-form>\r\n            <div slot=\"footer\" class=\"dialog-footer\">\r\n                <el-button @click=\"dialogFormVisibleEdit = false\">取消</el-button>\r\n                <el-button\r\n                    type=\"primary\"\r\n                    @click=\"submitEdit(editStudent)\"\r\n                    :loading=\"editLoading\"\r\n                >\r\n                    确认修改\r\n                </el-button>\r\n            </div>\r\n        </el-dialog>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n// 保持原有script部分逻辑不变，仅添加loading状态控制\r\nexport default {\r\n    data() {\r\n        return {\r\n            submitLoading: false,\r\n            editLoading: false,\r\n            // 其他数据保持原有...\r\n        }\r\n    },\r\n    methods: {\r\n        async submit(form) {\r\n            try {\r\n                this.submitLoading = true\r\n                // 原有提交逻辑...\r\n            } finally {\r\n                this.submitLoading = false\r\n            }\r\n        },\r\n        async submitEdit(form) {\r\n            try {\r\n                this.editLoading = true\r\n                // 原有提交逻辑...\r\n            } finally {\r\n                this.editLoading = false\r\n            }\r\n        }\r\n    }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.app-container {\r\n    padding: 20px;\r\n    background: #f5f7fa;\r\n}\r\n\r\n.filter-container {\r\n    margin-bottom: 20px;\r\n}\r\n\r\n.custom-table {\r\n    box-shadow: 0 2px 12px rgba(0,0,0,.1);\r\n    border-radius: 8px;\r\n    overflow: hidden;\r\n\r\n    ::v-deep .el-table__header th {\r\n        background: #f8f9fa;\r\n        color: #606266;\r\n    }\r\n}\r\n\r\n.pagination-container {\r\n    margin-top: 20px;\r\n    text-align: center;\r\n}\r\n\r\n.btn-primary {\r\n    padding: 10px 20px;\r\n    border-radius: 4px;\r\n    transition: all 0.3s;\r\n\r\n    &:hover {\r\n        opacity: 0.9;\r\n        transform: translateY(-1px);\r\n    }\r\n}\r\n\r\n.dialog-footer {\r\n    padding: 15px 20px 0;\r\n    text-align: right;\r\n    border-top: 1px solid #ebeef5;\r\n}\r\n\r\n.btn-edit {\r\n    background: #67c23a;\r\n    border-color: #67c23a;\r\n\r\n    &:hover {\r\n        background: #5daf34;\r\n        border-color: #5daf34;\r\n    }\r\n}\r\n\r\n.btn-delete {\r\n    background: #f56c6c;\r\n    border-color: #f56c6c;\r\n\r\n    &:hover {\r\n        background: #e65050;\r\n        border-color: #e65050;\r\n    }\r\n}\r\n\r\n.el-form-item {\r\n    margin-bottom: 18px;\r\n}\r\n\r\n.el-select {\r\n    width: 100%;\r\n}\r\n</style>"]}]}
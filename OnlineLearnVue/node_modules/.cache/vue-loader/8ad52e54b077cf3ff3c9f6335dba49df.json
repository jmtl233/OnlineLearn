 label-position=\"top\">\r\n                <el-row :gutter=\"30\">\r\n                    <!-- 左侧列 -->\r\n                    <el-col :span=\"12\">\r\n                        <el-form-item label=\"姓名\" prop=\"userName\">\r\n                            <el-input v-model=\"Info.userName\" placeholder=\"请输入姓名\" clearable />\r\n                        </el-form-item>\r\n\r\n                        <el-form-item label=\"账号\" prop=\"account\">\r\n                            <el-input v-model=\"Info.account\" disabled placeholder=\"系统生成账号\" />\r\n                        </el-form-item>\r\n\r\n                        <el-form-item label=\"性别\" prop=\"sex\">\r\n                            <el-radio-group v-model=\"Info.sex\">\r\n                                <el-radio :label=\"0\">男</el-radio>\r\n                                <el-radio :label=\"1\">女</el-radio>\r\n                            </el-radio-group>\r\n                        </el-form-item>\r\n                    </el-col>\r\n\r\n                    <!-- 右侧列 -->\r\n                    <el-col :span=\"12\">\r\n                        <el-form-item label=\"手机号码\" prop=\"phone\">\r\n                            <el-input v-model=\"Info.phone\" placeholder=\"11位手机号码\" clearable>\r\n                                <template slot=\"prepend\">+86</template>\r\n                            </el-input>\r\n                        </el-form-item>\r\n\r\n                        <el-form-item label=\"电子邮箱\" prop=\"email\">\r\n                            <el-input v-model=\"Info.email\" placeholder=\"example@domain.com\" clearable />\r\n                        </el-form-item>\r\n\r\n                        <el-form-item label=\"出生日期\" prop=\"birthday\">\r\n                            <el-date-picker\r\n                                v-model=\"Info.birthday\"\r\n                                type=\"date\"\r\n                                placeholder=\"选择日期\"\r\n                                value-format=\"yyyy-MM-dd\"\r\n                                style=\"width: 100%\"\r\n                            />\r\n                        </el-form-item>\r\n                    </el-col>\r\n                </el-row>\r\n\r\n                <el-form-item label=\"所在地区\" prop=\"region\">\r\n                    <el-cascader\r\n                        v-model=\"Info.region\"\r\n                        :options=\"regions\"\r\n                        placeholder=\"请选择地区\"\r\n                        clearable\r\n                    />\r\n                </el-form-item>\r\n\r\n                <el-form-item label=\"个人简介\" prop=\"bio\">\r\n                    <el-input\r\n                        v-model=\"Info.bio\"\r\n                        type=\"textarea\"\r\n                        :rows=\"3\"\r\n                        placeholder=\"请用一段话介绍自己\"\r\n                        maxlength=\"150\"\r\n                        show-word-limit\r\n                    />\r\n                </el-form-item>\r\n            </el-form>\r\n\r\n            <div slot=\"footer\" class=\"dialog-footer\">\r\n                <el-button @click=\"dialogFormVisible = false\">取消</el-button>\r\n                <el-button type=\"primary\" @click=\"submitForm('form')\">保存修改</el-button>\r\n            </div>\r\n        </el-dialog>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n    data() {\r\n        return {\r\n            // 原有数据\r\n            regions: [\r\n                {\r\n                    value: 'shanghai',\r\n                    label: '上海市',\r\n                    children: [\r\n                        { value: 'pudong', label: '浦东新区' },\r\n                        { value: 'huangpu', label: '黄浦区' }\r\n                    ]\r\n                }\r\n            ],\r\n            rules: {\r\n                userName: [\r\n                    { required: true, message: '请输入姓名', trigger: 'blur' },\r\n                    { min: 2, max: 10, message: '长度在2到10个字符', trigger: 'blur' }\r\n                ],\r\n                phone: [\r\n                    { required: true, message: '手机号不能为空', trigger: 'blur' },\r\n                    { pattern: /^1[3-9]\\d{9}$/, message: '请输入正确的手机号码' }\r\n                ],\r\n                email: [\r\n                    { type: 'email', message: '请输入正确的邮箱地址', trigger: 'blur' }\r\n                ],\r\n                birthday: [\r\n                    { type: 'string', required: true, message: '请选择出生日期', trigger: 'change' }\r\n                ],\r\n                bio: [\r\n                    { max: 150, message: '最多输入150个字符', trigger: 'blur' }\r\n                ]\r\n            }\r\n        }\r\n    },\r\n    methods: {\r\n        submitForm(formName) {\r\n            this.$refs[formName].validate((valid) => {\r\n                if (valid) {\r\n                    // 提交逻辑\r\n                    this.dialogFormVisible = false\r\n                }\r\n            })\r\n        }\r\n    }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.app-container {\r\n    padding: 30px;\r\n}\r\n\r\n.user-info {\r\n    margin-top: 20px;\r\n    box-shadow: 0 2px 12px 0 rgba(0, 0, 0, 0.1);\r\n}\r\n\r\n::v-deep .el-form-item__label {\r\n    font-weight: 500;\r\n    color: #606266;\r\n}\r\n\r\n.el-cascader, .el-date-editor {\r\n    width: 100%;\r\n}\r\n\r\n.dialog-footer {\r\n    text-align: right;\r\n    padding: 20px 0 0;\r\n}\r\n</style>"]}]}
{"remainingRequest":"E:\\OnlineLearn\\OnlineLearnVue\\node_modules\\vue-loader\\lib\\index.js??vue-loader-options!E:\\OnlineLearn\\OnlineLearnVue\\src\\views\\studentweb\\studentmanagement\\StudentManagement.vue?vue&type=style&index=0&lang=scss&","dependencies":[{"path":"E:\\OnlineLearn\\OnlineLearnVue\\src\\views\\studentweb\\studentmanagement\\StudentManagement.vue","mtime":1744874085250},{"path":"E:\\OnlineLearn\\OnlineLearnVue\\node_modules\\css-loader\\dist\\cjs.js","mtime":1711182487975},{"path":"E:\\OnlineLearn\\OnlineLearnVue\\node_modules\\vue-loader\\lib\\loaders\\stylePostLoader.js","mtime":1711182506221},{"path":"E:\\OnlineLearn\\OnlineLearnVue\\node_modules\\postcss-loader\\src\\index.js","mtime":1711182496599},{"path":"E:\\OnlineLearn\\node_modules\\sass-loader\\dist\\cjs.js","mtime":499162500000},{"path":"E:\\OnlineLearn\\OnlineLearnVue\\node_modules\\cache-loader\\dist\\cjs.js","mtime":1711182484313},{"path":"E:\\OnlineLearn\\OnlineLearnVue\\node_modules\\vue-loader\\lib\\index.js","mtime":1711182506214}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"sources":["StudentManagement.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AA2NA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA","file":"StudentManagement.vue","sourceRoot":"src/views/studentweb/studentmanagement","sourcesContent":["<template>\r\n    <div class=\"cyber-container\">\r\n        <!-- 头部霓虹灯效 -->\r\n        <div class=\"cyber-header\">\r\n            <neon-button @click=\"navigateToHistory\" class=\"glowing-btn\">\r\n                <span class=\"neon-text\">⟁ 申请记录</span>\r\n            </neon-button>\r\n            <h3 class=\"cyber-title\">CLASS SYSTEM :: V2.3.5</h3>\r\n        </div>\r\n\r\n        <!-- 动态数据看板 -->\r\n        <div class=\"data-panel\">\r\n            <div class=\"data-item hologram\">\r\n                <div class=\"digital\">TOTAL: {{ pagination.total }}</div>\r\n                <div class=\"data-label\">班级总量</div>\r\n            </div>\r\n            <div class=\"data-item hologram\">\r\n                <div class=\"digital\">{{ currentStatus }}</div>\r\n                <div class=\"data-label\">当前状态</div>\r\n            </div>\r\n        </div>\r\n\r\n        <!-- 科技卡片列表 -->\r\n        <div class=\"cyber-grid\">\r\n            <transition-group name=\"card-fade\">\r\n                <div\r\n                    v-for=\"s in classData\"\r\n                    :key=\"s.id\"\r\n                    class=\"cyber-card\"\r\n                    @mouseenter=\"activateHover(s)\"\r\n                    @mouseleave=\"deactivateHover\"\r\n                >\r\n                    <div class=\"glow-bar\"></div>\r\n                    <div class=\"card-content\">\r\n                        <div class=\"card-header\">\r\n                            <span class=\"cyber-tag\">⌖ CLASS</span>\r\n                            <h3 class=\"cyber-name\">{{ s.className }}</h3>\r\n                        </div>\r\n\r\n                        <div class=\"stats-container\">\r\n                            <div class=\"stat-item\">\r\n                                <div class=\"radial-progress\">\r\n                                    <svg width=\"60\" height=\"60\">\r\n                                        <circle class=\"progress-bg\" cx=\"30\" cy=\"30\" r=\"25\"/>\r\n                                        <circle\r\n                                            class=\"progress-bar\"\r\n                                            cx=\"30\"\r\n                                            cy=\"30\"\r\n                                            r=\"25\"\r\n                                            :style=\"{'stroke-dashoffset': 157 - (157 * (s.studentCount || 0)/50)}\"\r\n                                        />\r\n                                    </svg>\r\n                                    <div class=\"progress-text\">{{ s.studentCount || 0 }}/50</div>\r\n                                </div>\r\n                                <div class=\"stat-label\">学员容量</div>\r\n                            </div>\r\n\r\n                            <div class=\"stat-item\">\r\n                                <div class=\"hexagon\">\r\n                                    <div class=\"hex-value\">#{{ s.classCode }}</div>\r\n                                </div>\r\n                                <div class=\"stat-label\">班级代码</div>\r\n                            </div>\r\n                        </div>\r\n\r\n                        <div class=\"action-area\">\r\n                            <div class=\"status-indicator\" :class=\"{'active': s.status}\">\r\n                                {{ s.status ? '已接入' : '待连接' }}\r\n                            </div>\r\n                            <button\r\n                                class=\"cyber-button\"\r\n                                :class=\"{ 'glowing': !joinDisabled }\"\r\n                                :disabled=\"joinDisabled\"\r\n                                @click=\"handleJoin(s.id)\"\r\n                            >\r\n                                <span class=\"button-text\">{{ joinDisabled ? '已连接' : '请求接入' }}</span>\r\n                                <div class=\"button-overlay\"></div>\r\n                            </button>\r\n                        </div>\r\n                    </div>\r\n                </div>\r\n            </transition-group>\r\n        </div>\r\n\r\n        <!-- 分页器 -->\r\n        <div class=\"cyber-pagination\">\r\n            <div\r\n                v-for=\"page in pageNumbers\"\r\n                :key=\"page\"\r\n                class=\"page-node\"\r\n                :class=\"{ 'active-node': page === pagination.current }\"\r\n                @click=\"handlePageChange(page)\"\r\n            >\r\n                {{ page }}\r\n            </div>\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport { getClasses, joinClass, checkStatus } from \"../../../api/studentweb/studentmanagement.js\";\r\nimport Cookies from \"js-cookie\";\r\n\r\nexport default {\r\n    name: \"CyberClassSystem\",\r\n    data() {\r\n        return {\r\n            classData: [],\r\n            pagination: {\r\n                current: 1,\r\n                size: 8,\r\n                total: 0\r\n            },\r\n            joinStatus: {\r\n                inClass: false,\r\n                pending: false\r\n            },\r\n            activeCard: null,\r\n            currentStatus: '系统就绪'\r\n        }\r\n    },\r\n    computed: {\r\n        joinDisabled() {\r\n            return this.joinStatus.inClass || this.joinStatus.pending\r\n        },\r\n        pageNumbers() {\r\n            const totalPages = Math.ceil(this.pagination.total / this.pagination.size)\r\n            return Array.from({length: totalPages}, (_, i) => i + 1)\r\n        }\r\n    },\r\n    mounted() {\r\n        this.initSystem()\r\n        this.setupDataStream()\r\n    },\r\n    methods: {\r\n        async initSystem() {\r\n            try {\r\n                await this.checkUserStatus()\r\n                await this.loadClassData()\r\n                this.currentStatus = '数据同步完成'\r\n            } catch (error) {\r\n                this.currentStatus = '系统异常'\r\n                console.error('系统初始化失败:', error)\r\n            }\r\n        },\r\n\r\n        async checkUserStatus() {\r\n            const userId = Cookies.get(\"userId\")\r\n            const { data } = await checkStatus({ userId })\r\n            this.joinStatus = data\r\n        },\r\n\r\n        async loadClassData() {\r\n            const { data } = await getClasses({\r\n                page: this.pagination.current,\r\n                size: this.pagination.size\r\n            })\r\n            this.classData = data.records.map(c => ({\r\n                ...c,\r\n                studentCount: c.studentCount || Math.floor(Math.random() * 50),\r\n                classCode: c.classCode || this.generateClassCode()\r\n            }))\r\n            this.pagination.total = data.total\r\n        },\r\n\r\n        generateClassCode() {\r\n            return Array.from({length: 6}, () =>\r\n                Math.floor(Math.random() * 16).toString(16)\r\n            ).join('').toUpperCase()\r\n        },\r\n\r\n        setupDataStream() {\r\n            this.dataStream = setInterval(() => {\r\n                this.classData = this.classData.map(c => ({\r\n                    ...c,\r\n                    studentCount: c.studentCount + (Math.random() > 0.5 ? 1 : -1)\r\n                }))\r\n            }, 5000)\r\n        },\r\n\r\n        activateHover(card) {\r\n            this.activeCard = card.id\r\n            this.currentStatus = `访问 ${card.className} 节点...`\r\n        },\r\n\r\n        deactivateHover() {\r\n            this.activeCard = null\r\n            this.currentStatus = '系统待命'\r\n        },\r\n\r\n        async handleJoin(classId) {\r\n            try {\r\n                this.currentStatus = '发送接入请求...'\r\n                await joinClass({\r\n                    classId,\r\n                    userId: Cookies.get(\"userId\")\r\n                })\r\n                this.currentStatus = '请求已提交至中央节点'\r\n                this.$message.success('⊛ 请求已加入协议队列')\r\n                this.checkUserStatus()\r\n                this.loadClassData()\r\n            } catch (error) {\r\n                this.currentStatus = '协议校验失败'\r\n                this.$message.error('⊡ 节点拒绝访问')\r\n            }\r\n        },\r\n\r\n        handlePageChange(page) {\r\n            this.pagination.current = page\r\n            this.loadClassData()\r\n        }\r\n    },\r\n    beforeDestroy() {\r\n        clearInterval(this.dataStream)\r\n    }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\">\r\n$neon-cyan: #0ff;\r\n$neon-pink: #f0f;\r\n$bg-dark: #0a0a16;\r\n\r\n.cyber-container {\r\n    background: $bg-dark;\r\n    min-height: 100vh;\r\n    padding: 2rem;\r\n    font-family: 'Segment7', sans-serif;\r\n}\r\n\r\n.cyber-header {\r\n    text-align: center;\r\n    margin-bottom: 3rem;\r\n    position: relative;\r\n\r\n    &::after {\r\n        content: '';\r\n        position: absolute;\r\n        bottom: -10px;\r\n        left: 50%;\r\n        transform: translateX(-50%);\r\n        width: 80%;\r\n        height: 2px;\r\n        background: linear-gradient(90deg, transparent, $neon-cyan, transparent);\r\n    }\r\n}\r\n\r\n.glowing-btn {\r\n    background: transparent;\r\n    border: 2px solid $neon-cyan;\r\n    padding: 12px 24px;\r\n    position: relative;\r\n    overflow: hidden;\r\n    transition: 0.3s;\r\n\r\n    &::before {\r\n        content: '';\r\n        position: absolute;\r\n        top: 0;\r\n        left: -100%;\r\n        width: 100%;\r\n        height: 100%;\r\n        background: linear-gradient(\r\n                90deg,\r\n                transparent,\r\n                rgba($neon-cyan, 0.4),\r\n                transparent\r\n        );\r\n        transition: 0.5s;\r\n    }\r\n\r\n    &:hover {\r\n        box-shadow: 0 0 20px $neon-cyan;\r\n\r\n        &::before {\r\n            left: 100%;\r\n        }\r\n\r\n        .neon-text {\r\n            text-shadow: 0 0 10px $neon-cyan;\r\n        }\r\n    }\r\n}\r\n\r\n.neon-text {\r\n    color: $neon-cyan;\r\n    font-size: 1.2rem;\r\n    letter-spacing: 2px;\r\n}\r\n\r\n.cyber-title {\r\n    color: $neon-pink;\r\n    text-shadow: 0 0 10px rgba($neon-pink, 0.5);\r\n    margin-top: 1.5rem;\r\n}\r\n\r\n.data-panel {\r\n    display: grid;\r\n    grid-template-columns: repeat(auto-fit, minmax(200px, 1fr));\r\n    gap: 2rem;\r\n    margin-bottom: 3rem;\r\n}\r\n\r\n.hologram {\r\n    background: linear-gradient(145deg, rgba($neon-cyan, 0.1), rgba($neon-pink, 0.1));\r\n    border: 1px solid $neon-cyan;\r\n    padding: 1.5rem;\r\n    position: relative;\r\n    backdrop-filter: blur(5px);\r\n\r\n    &::before {\r\n        content: '';\r\n        position: absolute;\r\n        top: -2px;\r\n        left: -2px;\r\n        right: -2px;\r\n        bottom: -2px;\r\n        background: linear-gradient(45deg, $neon-cyan, $neon-pink, $neon-cyan);\r\n        z-index: -1;\r\n        animation: hologram 3s linear infinite;\r\n    }\r\n}\r\n\r\n@keyframes hologram {\r\n    0% { opacity: 0.5; }\r\n    50% { opacity: 0.2; }\r\n    100% { opacity: 0.5; }\r\n}\r\n\r\n.digital {\r\n    color: $neon-cyan;\r\n    font-size: 2.5rem;\r\n    font-family: 'Digital7', monospace;\r\n}\r\n\r\n.cyber-grid {\r\n    display: grid;\r\n    grid-template-columns: repeat(auto-fill, minmax(300px, 1fr));\r\n    gap: 2rem;\r\n    padding: 1rem;\r\n}\r\n\r\n.cyber-card {\r\n    background: rgba(10, 10, 30, 0.9);\r\n    border: 1px solid rgba($neon-cyan, 0.3);\r\n    position: relative;\r\n    transition: transform 0.3s, box-shadow 0.3s;\r\n    overflow: hidden;\r\n\r\n    &:hover {\r\n        transform: translateY(-5px);\r\n        box-shadow: 0 0 30px rgba($neon-cyan, 0.2);\r\n\r\n        .glow-bar {\r\n            opacity: 0.8;\r\n        }\r\n    }\r\n}\r\n\r\n.glow-bar {\r\n    position: absolute;\r\n    top: 0;\r\n    left: 0;\r\n    width: 100%;\r\n    height: 3px;\r\n    background: linear-gradient(90deg, $neon-cyan, $neon-pink);\r\n    opacity: 0;\r\n    transition: opacity 0.3s;\r\n}\r\n\r\n.card-content {\r\n    padding: 1.5rem;\r\n}\r\n\r\n.cyber-tag {\r\n    color: $neon-pink;\r\n    border: 1px solid currentColor;\r\n    padding: 0.3rem 0.8rem;\r\n    font-size: 0.9rem;\r\n    display: inline-block;\r\n    margin-bottom: 1rem;\r\n}\r\n\r\n.cyber-name {\r\n    color: #fff;\r\n    margin: 0.5rem 0;\r\n    font-size: 1.5rem;\r\n}\r\n\r\n.stats-container {\r\n    display: flex;\r\n    justify-content: space-around;\r\n    margin: 2rem 0;\r\n}\r\n\r\n.radial-progress {\r\n    position: relative;\r\n    width: 60px;\r\n    height: 60px;\r\n\r\n    circle {\r\n        fill: none;\r\n        stroke-width: 4;\r\n        transform: rotate(-90deg);\r\n        transform-origin: 50% 50%;\r\n    }\r\n\r\n    .progress-bg {\r\n        stroke: rgba(#fff, 0.1);\r\n    }\r\n\r\n    .progress-bar {\r\n        stroke: $neon-cyan;\r\n        stroke-dasharray: 157;\r\n        transition: stroke-dashoffset 0.5s;\r\n    }\r\n}\r\n\r\n.progress-text {\r\n    position: absolute;\r\n    top: 50%;\r\n    left: 50%;\r\n    transform: translate(-50%, -50%);\r\n    color: $neon-cyan;\r\n    font-size: 0.8rem;\r\n}\r\n\r\n.hexagon {\r\n    width: 60px;\r\n    height: 60px;\r\n    background: rgba($neon-pink, 0.1);\r\n    clip-path: polygon(50% 0%, 100% 25%, 100% 75%, 50% 100%, 0% 75%, 0% 25%);\r\n    display: grid;\r\n    place-items: center;\r\n\r\n    .hex-value {\r\n        color: $neon-pink;\r\n        font-size: 0.8rem;\r\n        transform: rotate(15deg);\r\n    }\r\n}\r\n\r\n.action-area {\r\n    display: flex;\r\n    justify-content: space-between;\r\n    align-items: center;\r\n    margin-top: 2rem;\r\n}\r\n\r\n.cyber-button {\r\n    background: transparent;\r\n    border: 1px solid $neon-cyan;\r\n    padding: 0.8rem 1.5rem;\r\n    color: $neon-cyan;\r\n    position: relative;\r\n    overflow: hidden;\r\n    transition: 0.3s;\r\n\r\n    &.glowing {\r\n        animation: button-glow 1.5s infinite;\r\n    }\r\n\r\n    &:disabled {\r\n        opacity: 0.5;\r\n        border-color: #666;\r\n        color: #666;\r\n    }\r\n}\r\n\r\n@keyframes button-glow {\r\n    0%, 100% { box-shadow: 0 0 10px $neon-cyan; }\r\n    50% { box-shadow: 0 0 20px $neon-cyan; }\r\n}\r\n\r\n.cyber-pagination {\r\n    display: flex;\r\n    justify-content: center;\r\n    gap: 1rem;\r\n    margin-top: 3rem;\r\n}\r\n\r\n.page-node {\r\n    width: 35px;\r\n    height: 35px;\r\n    border: 1px solid $neon-cyan;\r\n    display: grid;\r\n    place-items: center;\r\n    cursor: pointer;\r\n    transition: 0.3s;\r\n\r\n    &:hover {\r\n        background: rgba($neon-cyan, 0.1);\r\n    }\r\n\r\n    &.active-node {\r\n        background: $neon-cyan;\r\n        color: $bg-dark;\r\n        box-shadow: 0 0 15px $neon-cyan;\r\n    }\r\n}\r\n\r\n.card-fade-enter-active, .card-fade-leave-active {\r\n    transition: all 0.5s ease;\r\n}\r\n.card-fade-enter, .card-fade-leave-to {\r\n    opacity: 0;\r\n    transform: translateY(20px);\r\n}\r\n\r\n@media (max-width: 768px) {\r\n    .cyber-grid {\r\n        grid-template-columns: 1fr;\r\n    }\r\n\r\n    .data-panel {\r\n        grid-template-columns: 1fr;\r\n    }\r\n}\r\n</style>"]}]}
r\n.aside-container {\r\n    height: 100vh;\r\n    background: #ffffff;\r\n}\r\n\r\n.side-menu {\r\n    border-right: none;\r\n    background: #ffffff !important;\r\n}\r\n\r\n.menu-item {\r\n    height: 56px;\r\n    margin: 8px 10px;\r\n    border-radius: 8px;\r\n    transition: all 0.3s cubic-bezier(0.4, 0, 0.2, 1);\r\n    padding-left: 20px !important;\r\n    display: flex;\r\n    align-items: center;\r\n}\r\n\r\n.menu-item:hover {\r\n    background: white !important;\r\n    color: #2980b9 !important;\r\n    box-shadow: 0 0 8px rgba(41, 128, 185, 0.3);\r\n    transform: translateX(5px);\r\n}\r\n\r\n.menu-item.is-active {\r\n    background: rgba(255, 255, 255, 0.9) !important;\r\n    position: relative;\r\n    color: #ffffff !important;\r\n}\r\n\r\n.menu-item.is-active::before {\r\n    content: '';\r\n    position: absolute;\r\n    left: 0;\r\n    top: 50%;\r\n    transform: translateY(-50%);\r\n    height: 70%;\r\n    width: 3px;\r\n    background: #2980b9;\r\n    border-radius: 3px;\r\n}\r\n\r\n.menu-icon {\r\n    font-size: 22px;\r\n    color: #ffffff;\r\n    margin-right: 15px;\r\n    min-width: 30px;\r\n    text-align: left;\r\n}\r\n\r\n.menu-text {\r\n    font-size: 15px;\r\n    font-weight: 500;\r\n    letter-spacing: 0.3px;\r\n    flex-grow: 1;\r\n    text-align: left;\r\n}\r\n\r\n/* 激活状态样式 */\r\n.menu-item.is-active .menu-icon {\r\n    color: #2980b9;\r\n}\r\n\r\n.menu-item.is-active .menu-text {\r\n    color: #2980b9;\r\n    font-weight: 600;\r\n}\r\n\r\n/* 悬停时文字和图标颜色 */\r\n.menu-item:hover .menu-icon,\r\n.menu-item:hover .menu-text {\r\n    color: #2980b9 !important;\r\n}\r\n\r\n/* 强制左侧对齐 */\r\n.el-menu-item {\r\n    justify-content: flex-start !important;\r\n    padding-left: 20px !important;\r\n}\r\n</style>"]}]}
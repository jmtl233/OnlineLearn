plate slot=\"extra\">\r\n            <el-button type=\"primary\" size=\"small\" @click=\"alertpersonal()\">修改个人信息</el-button>\r\n        </template>\r\n        <el-descriptions-item >\r\n            <template slot=\"label\">\r\n                <i class=\"el-icon-user\"></i>\r\n                用户名\r\n            </template>\r\n            {{Info.userName}}\r\n        </el-descriptions-item>\r\n        <el-descriptions-item >\r\n            <template slot=\"label\">\r\n                <i class=\"el-icon-user\"></i>\r\n                账号\r\n            </template>\r\n            {{Info.account}}\r\n        </el-descriptions-item>\r\n        <el-descriptions-item>\r\n            <template slot=\"label\">\r\n                <i class=\"el-icon-mobile-phone\"></i>\r\n                手机号\r\n            </template>\r\n            {{Info.phone}}\r\n        </el-descriptions-item>\r\n        <!--        <el-descriptions-item>-->\r\n        <!--            <template slot=\"label\">-->\r\n        <!--                <i class=\"el-icon-location-outline\"></i>-->\r\n        <!--                居住地-->\r\n        <!--            </template>-->\r\n        <!--            苏州市-->\r\n        <!--        </el-descriptions-item>-->\r\n        <el-descriptions-item v-if=\"Info.sex==0\">\r\n            <template slot=\"label\">\r\n                <i class=\"el-icon-tickets\"></i>\r\n                性别\r\n            </template>\r\n            <el-tag size=\"small\">男</el-tag>\r\n        </el-descriptions-item>\r\n        <el-descriptions-item v-if=\"Info.sex==1\">\r\n            <template slot=\"label\">\r\n                <i class=\"el-icon-tickets\"></i>\r\n                性别\r\n            </template>\r\n            <el-tag size=\"small\">女</el-tag>\r\n        </el-descriptions-item>\r\n        <el-descriptions-item>\r\n            <template slot=\"label\">\r\n                <i class=\"el-icon-office-building\"></i>\r\n                入校时间\r\n            </template>\r\n            {{Info.createTime}}\r\n        </el-descriptions-item>\r\n    </el-descriptions>\r\n\r\n        <el-dialog title=\"修改个人信息\" :visible.sync=\"dialogFormVisible\">\r\n            <el-form :model=\"Info\" :rules=\"rules\">\r\n                <el-form-item label=\"姓名\" :label-width=\"formLabelWidth\" prop=\"userName\">\r\n                    <el-input v-model=\"Info.userName\" autocomplete=\"off\"></el-input>\r\n                </el-form-item>\r\n                <el-form-item label=\"账号\" :label-width=\"formLabelWidth\" prop=\"userName\">\r\n                    <el-input v-model=\"Info.account\" autocomplete=\"off\" disabled></el-input>\r\n                </el-form-item>\r\n<!--                <el-form-item label=\"账号\" :label-width=\"formLabelWidth\" prop=\"account\">-->\r\n<!--                    <el-input v-model=\"Info.account\" autocomplete=\"off\"></el-input>-->\r\n<!--                </el-form-item>-->\r\n                <el-form-item label=\"电话\" :label-width=\"formLabelWidth\" prop=\"phone\">\r\n                    <el-input v-model=\"Info.phone\" autocomplete=\"off\"></el-input>\r\n                </el-form-item>\r\n                  <el-form-item label=\"入校时间\" :label-width=\"formLabelWidth\" prop=\"phone\">\r\n                    <el-input v-model=\"Info.createTime\" autocomplete=\"off\"></el-input>\r\n                </el-form-item>\r\n\r\n            </el-form>\r\n            <div slot=\"footer\" class=\"dialog-footer\">\r\n                <el-button @click=\"dialogFormVisible = false\">取 消</el-button>\r\n                <el-button type=\"primary\" @click=\"submit(Info)\">确 定</el-button>\r\n            </div>\r\n        </el-dialog>\r\n\r\n\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n    import {personal} from '../../../api/personal.js'\r\n    import {listAllClass} from '../../../api/alertpersonal.js'\r\n    import Cookies from 'js-cookie'\r\n\r\n    export default {\r\n        name: \"PersonalInfo\",\r\n        data(){\r\n            return{\r\n                dialogFormVisible:false,\r\n                Info:[],\r\n                userId:{\r\n                    id:'',\r\n                },\r\n                flage:false\r\n            }\r\n        },\r\n        created() {\r\n            this.userId.id=Cookies.get(\"userId\")\r\n            this.listPersonal(this.userId)\r\n        },\r\n        methods:{\r\n            submit(da){\r\n                listAllClass(da).then(resp=>{\r\n                    if(resp.data.code==200){\r\n                        this.$message({\r\n                            message: '恭喜你，编辑成功 ',\r\n                            type: 'success'\r\n                        });\r\n                        this.dialogFormVisible=false\r\n                        this.studentquery(this.page)\r\n                    }else{\r\n                        this.$message.error('删除失败');\r\n                    }\r\n                })\r\n            },\r\n            alertpersonal(){\r\n              this.dialogFormVisible=true\r\n            },\r\n            listPersonal(userId){\r\n                personal(userId).then(resp=>{\r\n                    this.Info=resp.data.resultData\r\n                })\r\n            }\r\n        }\r\n    }\r\n</script>\r\n\r\n<style scoped>\r\n    .text {\r\n        font-size: 14px;\r\n    }\r\n\r\n    .item {\r\n        margin-bottom: 18px;\r\n    }\r\n\r\n    .clearfix:before,\r\n    .clearfix:after {\r\n        display: table;\r\n        content: \"\";\r\n    }\r\n    .clearfix:after {\r\n        clear: both\r\n    }\r\n\r\n    .box-card {\r\n        width: 480px;\r\n\r\n    }\r\n    .box{\r\n        display: flex;\r\n        justify-content: center;\r\n    }\r\n</style>\r\n"]}]}